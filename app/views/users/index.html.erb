<h1>Users Index</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>User name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to user.username, user %></td>
      </tr>
    <% end %>
  </tbody>
</table>